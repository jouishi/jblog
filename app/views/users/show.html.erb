<div class="main">
  <div class="inner">
    <div class="user__wrapper">
      <h2 class="page-heading">
        <%= @user.name%>
      </h2>
      <table class="table">
        <tbody>
          <tr>
            <th class="table__col1">name</th>
            <td class="table__col2"><%= @user.name %></td>
          </tr>
         
        </tbody>
      </table>
      <h2 class="page-heading">
        <%= @user.name%>'s blog
      </h2>
      <div class="user__card">
       <%= render partial: 'blogs/blog' , collection:@user.blogs %>
        <%# 部分テンプレートでそのユーザーが投稿したプロトタイプ投稿一覧を表示する %>
      </div>
    </div>
  </div>
</div>